<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mavc.parkingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkingApp">
        <activity android:name=".EntradaActivity"></activity>
        <activity android:name=".BahiaActivity" />
        <activity android:name=".TipovehiculoActivity" />
        <activity android:name=".TipbahiaActivity" />
        <activity android:name=".VehiculoActivity" />
        <activity android:name=".ClienteActivity" />
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".ZonaActivity" />
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.ParkingApp.NoActionBar" />
        <activity android:name=".MarcaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>